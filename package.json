{
  "name": "garyblackwood.co.uk",
  "private": true,
  "description": "The personal website of Gary Blackwood.",
  "version": "0.1.0",
  "author": "Gary Blackwood <gary@garyblackwood.co.uk>",
  "dependencies": {
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.1.1",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "^2.2.2",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.13",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-images": "^0.5.19",
    "react-photo-gallery": "^7.0.2",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-irving": "^0.16.19"
  },
  "devDependencies": {
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ public",
    "build": "gatsby build",
    "deploy": "scp -r public/* deploy@garyblackwood.co.uk:~/garyblackwood.co.uk",
    "develop": "gatsby develop",
    "format": "prettier --write *.js src/**/*.{js,css}",
    "lint:js": "eslint --fix *.js src/**/*.js",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}

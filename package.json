{
  "name": "garyblackwood.co.uk",
  "private": true,
  "description": "The personal website of Gary Blackwood.",
  "version": "0.1.0",
  "author": "Gary Blackwood <gary@garyblackwood.co.uk>",
  "dependencies": {
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-catch-links": "^2.3.5",
    "gatsby-plugin-feed": "^2.5.5",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-nprogress": "^2.3.4",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-postcss": "^2.3.4",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-plugin-typography": "^2.5.4",
    "gatsby-remark-autolink-headers": "^2.3.5",
    "gatsby-remark-copy-linked-files": "^2.3.5",
    "gatsby-remark-images": "^3.3.10",
    "gatsby-remark-prismjs": "^3.5.4",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^2.8.15",
    "gatsby-transformer-sharp": "^2.5.5",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-images": "^0.5.19",
    "react-photo-gallery": "^8.0.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-irving": "^0.16.19"
  },
  "devDependencies": {
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ public",
    "build": "gatsby build",
    "deploy": "scp -r public/* deploy@garyblackwood.co.uk:~/garyblackwood.co.uk",
    "develop": "gatsby develop",
    "format": "prettier --write *.js src/**/*.{js,css}",
    "lint:js": "eslint --fix *.js src/**/*.js",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
